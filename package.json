    "@babel/preset-env": "^7.10.4",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "babel-plugin",
    "babel",
    "plugin",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fqishuai/babel-plugin-starter.git"
  },
  "author": "fqishuai",
  "bugs": {
    "url": "https://github.com/fqishuai/babel-plugin-starter/issues"
  },
